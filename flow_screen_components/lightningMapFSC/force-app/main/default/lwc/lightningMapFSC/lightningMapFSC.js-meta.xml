<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="markerList" type="String[]" role="inputOnly" description="List of markers for display on screen"/>
            <property name="title" type="String" role="inputOnly" description="Title of the Map displayed."/>
            <property name="showListView" type="Boolean" role="inputOnly" description="Set to true to show location list.  Default is false"/>
            <property name="geolocation" type="Boolean" role="inputOnly" description="Set to true if you are passing in geolocation instead"/>
            <property name="selectedMarkerValue" type="String" role="outputOnly" description="Output of selected marker."/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>